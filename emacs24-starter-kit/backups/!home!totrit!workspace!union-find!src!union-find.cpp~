#include <assert.h>
#include <time.h>

#include <iostream>

public class UF {

public:
	int find(int e) {
	}

	void union(int e1, int e2) {
	}
};

int main() {
	srand(time(NULL));
	const int ARRAY_LEN = 1000;
	const int INT_RANGE = 100000;
	int *array = new int[ARRAY_LEN];
	for (int i = 0; i < ARRAY_LEN; i ++) {
		array[i] = rand() % INT_RANGE;
	}

}
